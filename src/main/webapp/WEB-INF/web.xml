<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>

  <servlet>
    <servlet-name>DisplayNumber</servlet-name>
    <servlet-class>DispNum</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>DisplayNumber</servlet-name>
    <url-pattern>/myNum</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>AddingNumbers</servlet-name>
    <servlet-class>AddNums</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>AddingNumbers</servlet-name>
    <url-pattern>/add</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>DisplayJSON</servlet-name>
    <servlet-class>RunJSON</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>DisplayJSON</servlet-name>
    <url-pattern>/json</url-pattern>
  </servlet-mapping>
</web-app>
